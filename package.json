{
  "name": "@darthsaul/vue-component-library",
  "version": "0.1.0",
  "description": "UI kit for Vue apps",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./styles/index.css"
  },
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=demo",
    "build": "npm run build --workspace=ui",
    "build:ui": "npm run build --workspace=ui",
    "build:demo": "npm run build --workspace=demo",
    "preview": "npm run preview --workspace=demo"
  },
  "keywords": [
    "vue",
    "component-library",
    "ui-kit",
    "vite"
  ],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "ui",
    "demo",
    "styles",
    "icons"
  ],
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  }
}
